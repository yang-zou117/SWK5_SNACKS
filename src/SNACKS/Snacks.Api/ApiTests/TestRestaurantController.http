@url = https://localhost:7000
@content-type = application/json

###

# register new restaurant, returns new apiKey

POST {{url}}/api/restaurant/register
Accept: {{content-type}}
Content-Type: {{content-type}}

{
  "restaurant": {
    "restaurantName": "Burgerei",
    "webhookUrl": "https://example.com/burgerei",
    "image": {
       "fileType": "png",
       "imageData": ""
    }
  },
  "address": {
    "zipcode": 4232,
    "city": "Hagenberg",
    "street": "Softwarepark",
    "streetNumber": 30,
    "gpsLongitude": 13.388860,
    "gpsLatitude": 52.516275
  },
  "openingHours": [
    {
      "weekDay": "Monday",
      "startTime": "08:00:00",
      "endTime": "17:00:00"
    },
    {
      "weekDay": "Tuesday",
      "startTime": "09:00:00",
      "endTime": "18:00:00"
    }
  ],
  "closingDays": [
    {
      "weekDay": "Saturday"
    },
    {
      "weekDay": "Sunday"
    }
  ]
}


###
# get address, opening hours and closing days of restaurant 1

GET {{url}}/api/restaurant/1
Accept: {{content-type}}


###

# get address, opening hours and closing days of restaurant 1000
# should return error as no restaurant with id 1000 exists

GET {{url}}/api/restaurant/1000
Accept: {{content-type}}


###

# insert new opening hours for restaurant 1

Post {{url}}/api/restaurant/openingHours/1
Accept: {{content-type}}
Content-Type: {{content-type}}
X-API-KEY: testApiKey123

{
    "weekDay": "Wednesday",
    "startTime": "08:00:00",
    "endTime": "10:00:00"
}
###

# insert invalid opening hours for restaurant 1
#  should return error as conflict with closing day

Post {{url}}/api/restaurant/openingHours/1
Accept: {{content-type}}
Content-Type: {{content-type}}
X-API-KEY: testApiKey123

{
    "weekDay": "Monday",
    "startTime": "08:00:00",
    "endTime": "10:00:00"
}


###

# delete opening hours with id 1
DELETE {{url}}/api/restaurant/openingHours/1/2
X-API-KEY: testApiKey123


###
# delete opening hours with id 1000
# should return error as no opening hours with id 1000 exists
DELETE {{url}}/api/restaurant/openingHours/1/1000
X-API-KEY: testApiKey123

### 

# post closing day for restaurant 3
POST {{url}}/api/restaurant/closingDay/3
Accept: {{content-type}}
X-API-KEY: testApiKey123
Content-Type: {{content-type}}

{
    "weekDay": "WednesDay"
}

###

# post closing day for restaurant 3
# should not work as closing day already exists

POST {{url}}/api/restaurant/closingDay/3
Accept: {{content-type}}
X-API-KEY: testApiKey123
Content-Type: {{content-type}}

{
    "weekDay": "Monday"
}

###

# delete closing day of restaurant 1 with Monday as weekDay
DELETE {{url}}/api/restaurant/closingDay/1/Tuesday
X-API-KEY: testApiKey123

### 

# delete closing day of restaurant 1 with Sunday as weekDay
# should return error as no closing day with Sunday as weekDay exists
DELETE {{url}}/api/restaurant/closingDay/1/Sunday
X-API-KEY: testApiKey123
    